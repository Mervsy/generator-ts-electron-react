{
  "name": "generator-ts-electron-react",
  "version": "0.0.0",
  "description": "Yeoman Generator that includes Electron, React, and TypeScript.",
  "homepage": "https://github.com/Mervsy/generator-ts-electron-react",
  "author": {
    "name": "Ryan Myrvold",
    "email": "ryan@mervsy.com",
    "url": "https://github.com/Mervsy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mervsy/generator-ts-electron-react.git"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "electron",
    "react",
    "typescript",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.9.1",
    "yeoman-assert": "^3.1.1",
    "eslint": "^5.15.3",
    "prettier": "^1.16.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-config-xo": "^0.26.0",
    "jest": "^24.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  }
}
